*
{
    margin: 0;
    padding: 0;
}
* body
{
    background: none repeat scroll 0 0 #cbd8e1;
    color: #444444;
    font: 0.75em/1.6em "Trebuchet MS" ,Tahoma,Verdana,Arial,Helvetica,sans-serif;
}
a
{
    background: inherit;
    color: #E60000;
    text-decoration: none;
}
a:hover
{
    background: inherit;
    color: #AA0001;
    text-decoration: underline;
}
p
{
    margin: 0 10px 10px;
}
h1
{
    margin: 20px 0 0;
}
h1 a
{
    background: inherit;
}
img
{
    border: 0 none;
}
h2
{
    font: 1.6em "Trebuchet MS" ,Arial,Sans-Serif;
    margin: 0 0 12px;
}
h3 a
{
    font-size: 1em;
}
#main h3 a
{
    color: #808080;
    font-size: 0.84em;
    font-weight: normal;
    text-transform: uppercase;
}
#content
{
    background: none repeat scroll 0 0 #FFFFFF;
    border-top: 5px solid #EEEEEE;
    margin: 0 auto;
    padding: 7px 0 0;
    width: 90%;
}

#top
{
    display: block;
    float: right;
    text-align: right;
}

#top a
{
    background: url("img/toparrow.png") no-repeat scroll left center #FFFFFF;
    color: #808080;
    margin: 0 0 0 13px;
    padding: 0 0 0 12px;
}
input.searchfield
{
    border: 2px solid #CCCCCC;
    font-size: 0.9em;
    padding: 6px;
    width: 175px;
}
input.searchbutton
{
    background: none repeat scroll 0 0 #254A86;
    border: 0 none;
    color: #FFFFFF;
    font-size: 0.9em;
    font-weight: bold;
    padding: 6px;
}
#logo
{
    color: #808080;
    float: left;
    line-height: 25px;
}
#menu
{
    background: url("img/menubg.png") repeat-x scroll 0 0 #FFFFFF;
    clear: both;
    height: 30px;
    margin: 0 0 5px;
    padding: 5px 0 1px 3px;
}
#menu li
{
    display: inline;
    font-size: 1.1em;
    text-transform: uppercase;
}
#menu li a
{
    color: #808080;
    margin: 0 5px 0 10px;
}
#menu li a:hover
{
    color: #444444;
}
#left
{
    background: none repeat scroll 0 0 #EEEEEE;
    clear: left;
    float: left;
    margin: 0 0 20px;
    padding: 10px 15px;
    width: 260px;
}
#left img
{
    margin: 0 0 8px;
}
#left li
{
    list-style: none outside none;
}
#left li a
{
    background: url("img/arrow.gif") no-repeat scroll left center transparent;
    color: #787A8F;
    font-weight: bold;
    padding: 0 0 0 12px;
}
#main
{
    float: left;
    margin: 11px 15px 15px;
    width: 98%;
}
/*
#main img {
    border: 3px solid #EEEEEE;
    float: left;
    margin: 0 15px 5px 0;
    padding: 1px;
}
*/
#line
{
    background: url("img/smalldot.gif") repeat-x scroll 0 0 #FFFFFF;
    clear: both;
    margin: 0 0 15px;
    padding: 3px;
}
#ad
{
    border: 3px solid #EEEEEE;
    color: #CCCCCC;
    float: right;
    height: 50px;
    margin: 0 0 15px;
    padding: 33px 0 0;
    text-align: center;
    width: 250px;
}
#shortnews
{
    float: right;
    width: 250px;
}
.news
{
    border-bottom: 1px solid #EEEEEE;
    margin: 0 0 10px;
    padding: 0 0 2px;
}
#right
{
    float: right;
    margin: 0 15px 20px 0;
    padding: 0 0 0 15px;
    text-align: justify;
    width: 620px;
}
.border
{
    border-left: 4px solid #CCCCCC;
    clear: both;
    padding: 0 0 0 15px;
}
#rl
{
    float: left;
    margin: 0 20px 30px 0;
    width: 290px;
}
#rr
{
    float: right;
    width: 290px;
}
#rl li, #rr li
{
    border-bottom: 1px solid #EEEEEE;
    list-style: none outside none;
    margin: 0;
    padding: 4px;
}
#rl li a, #rr li a
{
    background: url("img/arrow.gif") no-repeat scroll left center #FFFFFF;
    color: #808080;
    padding: 0 0 0 12px;
}
#footer
{
    background: none repeat scroll 0 0 #EEEEEE;
    clear: both;
    color: #808080;
    font-size: 0.9em;
    margin: 0 0 15px;
    padding: 10px;
}
.links a
{
    background: url("img/dot.png") no-repeat scroll left center transparent;
    color: #787A8F;
    font-weight: bold;
    margin: 0 10px 0 0;
    padding: 0 0 0 10px;
}
.sponsor
{
    background: none repeat scroll 0 0 #EEEEEE;
    color: red;
}
#footer li
{
    list-style: none outside none;
}
#fl
{
    width: 600px;
}
#fr
{
    float: right;
}

.divTituloPagina
{
    background-color: #EFEFEF;
    color: #FFFFFF;
    margin: 9px 0;
    padding: 10px 0 10px 22px;
}

.divTituloPagina h3
{
    color: #646a6f;
}

.placeholder
{
    color: #aaa;
}

.clear
{
    clear: both;
}

.hidden
{
    visibility: hidden;
}

div#title
{
    display: block;
    float: left;
    text-align: left;
}


/* Styles for validation helpers
-----------------------------------------------------------*/
.field-validation-error
{
    color: #ff0000;
}

.field-validation-valid
{
    display: none;
}

.input-validation-error
{
    border: 1px solid #ff0000;
    background-color: #ffeeee;
}

.validation-summary-errors
{
    font-weight: bold;
    color: #ff0000;
}

.validation-summary-valid
{
    display: none;
}


.circle
{
    background: none repeat scroll 0 0 black; /*border-radius: 64px 64px 64px 64px;*/
    color: white; /*height: 24px;*/
    padding: 2px;
    text-align: center; /*width: 24px;*/
    opacity: 0.5;
    filter: alpha(Opacity=50);
    border: 0px solid #ffffff;
    display: block;
}

.circle:hover
{
    background: none repeat scroll 0 0 black; /*border-radius: 64px 64px 64px 64px;*/
    color: white; /*height: 24px;*/
    padding: 2px;
    text-align: center; /* width: 24px;*/
    opacity: 0.9;
    filter: alpha(Opacity=90, FinishedOpacity=100);
    border: 0px solid #2D9F2D;
    display: block;
    cursor: pointer;
}


/* Erro e mensagens */
.info, .success, .warning, .error, .validation
{
    border: 1px solid;
    margin: 10px 0px;
    padding: 10px 10px 10px 50px;
    background-repeat: no-repeat;
    background-position: 10px center;
    font-size: 12px;
}
.info
{
    color: #00529B;
    background-color: #BDE5F8;
    background-image: url('../Content/img/info.png');
}
.success
{
    color: #4F8A10;
    background-color: #DFF2BF;
    background-image: url('../Content/img/success.png');
}
.warning
{
    color: #9F6000;
    background-color: #FEEFB3;
    background-image: url('../Content/img/warning.png');
}
.error
{
    color: #D8000C;
    background-color: #FFBABA;
    background-image: url('../Content/img/error.png');
}
/* Fim de div de erros */

div#grid
{
    margin: 0.5em 0 0;
}

.buttonAction
{
    background: none repeat scroll 0 0 #EFEFEF;
    border-radius: 4px 4px 4px 4px;
    margin: 1.5em 0 0;
    padding: 1.5em;
    text-align: right;
}

.buttonAction .primaryAction
{
    padding: 10px 22px;
    line-height: 1;
    background: #254a86;
    border: 1px solid #163362;
    font-size: 12px;
    font-weight: bold;
    color: #fff; /* CSS3 */
    border-radius: 4px;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    -o-border-radius: 4px;
    -khtml-border-radius: 4px;
    box-shadow: 1px 1px 0 #fff;
    -webkit-box-shadow: 1px 1px 0 #fff;
    -moz-box-shadow: 1px 1px 0 #fff;
    text-shadow: -1px -1px 0 rgba(0,0,0,.25);
}

.buttonAction .primaryAction:active
{
    position: relative;
    top: 1px;
}

.buttonAction .secondaryAction
{
    text-align: left !important;
}

.buttonAction button.secondaryAction
{
    background: transparent;
    border: none;
    color: #777;
    margin: 1.25em 0 0 0;
    padding: 0;
}


.btnEdit
{
    background: url("../Content/img/icon-edit.png") no-repeat;
    width: 22px;
    height: 22px;
    border: 0px none transparent;
    cursor: pointer;
}

.btnSave
{
    background: url("../Content/img/icon-save.png") no-repeat;
    width: 22px;
    height: 22px;
    border: 0px none transparent;
    cursor: pointer;
}

.btnUndo
{
    background: url("../Content/img/icon-undo.png") no-repeat;
    width: 22px;
    height: 22px;
    border: 0px none transparent;
    cursor: pointer;
}

.btnAddInfo
{
    background: url("../Content/img/add_info_22.png") no-repeat;
    width: 22px;
    height: 22px;
    border: 0px none transparent;
    cursor: pointer;
}

#infoLogin
{
    position: absolute;
    text-align: right;
    width: 100%;
    z-index: 999;
}

#btnInfo
{
    color: #164263;
    padding: 5px 5px 5px 5px;
}

#btnInfo label
{
    padding-right: 5px;
}

.divInfoLogin
{
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #DEDEDE;
    border-radius: 4px 4px 4px 4px;
    float: right;
    height: 155px;
    margin: -10px 10px -300px 0;
    padding: 10px;
    width: 200px;
    z-index: 999;
    display: block;
    position: relative;
}

.imgUserInfo
{
    background: url("../Content/img/userInfo.png") no-repeat;
    width: 22px;
    height: 22px;
    border: 0px none transparent;
    cursor: pointer;
    float: right;
    z-index: 999;
}

.imgUserInfoClick
{
    background: url("../Content/img/userInfoEnable.png") no-repeat;
    width: 22px;
    height: 22px;
    border: 0px none transparent;
    cursor: pointer;
    float: right;
}

.divInfoLogin #line
{
    background: url("img/smalldot.gif") repeat-x scroll 0 0 #FFFFFF;
    clear: both;
    margin: 0 0 0;
    padding: 3px;
    z-index: 999;
}


.textInputMenor
{
    width: 15% !important;
    color: rgb(175, 175, 175);
    border: 1px solid #AAAAAA;
    padding: 4px 2px;
}


fieldset
{
    border: 0px none;
    padding: 0 1.4em 0.4em 1.4em;
    margin: 0 0 1.5em 0;
}

legend
{
    font-size: 1.2em;
    font-weight: bold;
}

.popupForm{
    border-bottom: 1px solid #EFEFEF;
    padding: 1em;
}
    
.popupForm .textInput, .fileUpload, .selectInput, select, textarea
{
    float: left;
    width: 53%;
    margin: 0;
}

.popupForm .textInput, .inlineLabels .fileUpload, .inlineLabels .selectInput, .inlineLabels select, .inlineLabels textarea
{
    float: left;
    width: 60%; 
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #AAAAAA;
    padding: 4px 2px;
}

.popupForm .formHint{ clear: both; float: none; width: auto; margin-left: 34%; position: static; }

.popupForm label{ float: left; margin: .3em 2% 0 0; padding: 0; line-height: 1; position: relative; width: 32%; }
      
      
      
/* Modal Calendario*/
#mask {
  position:absolute;
  left:0;
  top:0;
  z-index:9000;
  background-color:#000;
  display:none;
}
  
#boxes .window {
  position:absolute;
  left:0;
  top:0;
  width:440px;
  display:none;
  z-index:9999;
  padding:20px;
}

#boxes .dialog {
  width:375px; 
  padding:10px;
  background-color:#ffffff;
}
  